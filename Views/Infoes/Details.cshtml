@model InfoSharing.Models.Info

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h1>Web_development</h1>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Question
        </dt>
        
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InfoQuestion)
        </dd>
        <hr>
        <dt class = "col-sm-2">
            Answer
        </dt>
        
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InfoAnswer)
        </dd>
        <hr>
    </dl>
</div>
<div>
    <a class="as_btn" asp-action="Edit" asp-route-id="@Model?.id">Edit</a> |
    <a class="as_btn" asp-action="Index">Back</a>
</div>
